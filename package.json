{
  "name": "ojo-paritario-front",
  "displayName": "Ojo Paritario",
  "description": "",
  "main": "index.js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:www": "BUILD_PATH='./www' react-scripts build",
    "eject": "react-scripts eject",
    "build:android": "npm run build:www && cordova build android",
    "build:ios": "npm run build:www && cordova build ios",
    "emulate:android": "npm run build:www && cordova emulate android",
    "emulate:ios": "npm run build:www && cordova emulate ios"
  },
  "homepage": ".",
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "cordova": {
    "platforms": [
      "android",
      "ios"
    ],
    "plugins": {
      "cordova-plugin-firebase-messaging": {
        "ANDROID_FIREBASE_BOM_VERSION": "30.3.1"
      }
    }
  },
  "dependencies": {
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "antd": "^4.16.13",
    "firebase": "^10.0.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "styled-components": "^5.3.1",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.7",
    "@types/react-router-dom": "^5.3.1",
    "@types/styled-components": "^5.1.13",
    "babel-plugin-styled-components": "^1.13.2",
    "cordova-android": "^12.0.0",
    "cordova-ios": "^7.0.0",
    "cordova-plugin-firebase-messaging": "^7.0.4",
    "cordova-support-android-plugin": "~2.0.4",
    "sass": "^1.38.2"
  }
}